{
  "name": "openseadragon-annotations",
  "version": "0.0.1",
  "description": "OpenSeadragon annotations plugin",
  "main": "openseadragon-annotations.js",
  "scripts": {
    "start": "karma start",
    "test": "karma start --single-run",
    "lint": "eslint ./src/*.js",
    "build": "parallelshell 'npm run lint -s' 'rimraf dist && mkdirp dist/img && cp ./img/*.* ./dist/img/' && parallelshell 'browserify -t [ babelify ] ./src/openseadragon-annotations.js > ./dist/openseadragon-annotations.js'  'browserify -d -t [ babelify ] -t [ uglifyify ] ./src/openseadragon-annotations.js | exorcist ./dist/openseadragon-annotations.min.map > ./dist/openseadragon-annotations.min.js'"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Emigre/openseadragon-annotations.git"
  },
  "author": "Gaspar Rey <info@gasparrey.com> (http://www.gasparrey.com/)",
  "license": "BSD-2-Clause",
  "bugs": {
    "url": "https://github.com/Emigre/openseadragon-annotations/issues"
  },
  "keywords": [
    "deep",
    "zoom",
    "images",
    "dzi",
    "annotations",
    "openseadragon"
  ],
  "devDependencies": {
    "babel": "^5.8.5",
    "babelify": "^6.1.3",
    "browserify": "^11.0.0",
    "chai": "^3.2.0",
    "chai-as-promised": "^5.1.0",
    "eslint": "^0.24.1",
    "exorcist": "^0.4.0",
    "karma": "^0.13.3",
    "karma-browserify": "^4.2.1",
    "karma-chai": "^0.1.0",
    "karma-chai-as-promised": "^0.1.2",
    "karma-mocha": "^0.2.0",
    "karma-mocha-reporter": "^1.0.2",
    "karma-phantomjs-launcher": "^0.2.0",
    "karma-sinon": "^1.0.4",
    "mkdirp": "^0.5.1",
    "mocha": "^2.2.5",
    "node-cp": "^0.1.1",
    "parallelshell": "^1.2.0",
    "phantomjs": "^1.9.17",
    "rimraf": "^2.4.2",
    "sinon": "^1.15.4",
    "uglifyify": "^3.0.1"
  },
  "homepage": "https://github.com/Emigre/openseadragon-annotations"
}
