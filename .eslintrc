{
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true,
    "mocha": true,
    "es6": true
  },
  "globals": {
    "OpenSeadragon": 1,
    "expect": 1
  },
  "ecmaFeatures": {
    "modules": true
  },
  "rules": {
    "brace-style": [1, "1tbs", { "allowSingleLine": true }],
    "camelcase": 0,
    "comma-style": [1, "last"],
    "comma-spacing": 1,
    "computed-property-spacing": [0, "always"],
    "eqeqeq": 0,
    "global-strict": 0,
    "indent": [1, 4],
    "key-spacing": 1,
    "no-alert": 1,
    "no-console": 1,
    "no-debugger": 1,
    "no-empty": 0,
    "no-extra-bind": 1,
    "no-trailing-spaces": 1,
    "no-undef": 1,
    "no-underscore-dangle": 0,
    "no-unused-expressions": 0,
    "no-unused-vars": [1],
    "no-use-before-define": 0,
    "object-curly-spacing": [2, "always"],
    "quotes": [0],
    "semi": 1,
    "space-after-keywords": [2, "always"],
    "space-before-function-paren": [1, {"anonymous": "always", "named": "never"}],
    "space-in-parens": [1, "never"],
    "strict": 0
  }
}
