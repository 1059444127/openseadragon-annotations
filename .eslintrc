{
  "env": {
    "browser": true,
    "mocha": true,
    "es6": true
  },
  "globals": {
    "OpenSeadragon": 1,
    "expect": 1
  },
  "ecmaFeatures": {
    "modules": true
  },
  "rules": {
    "brace-style": [1, "1tbs", { "allowSingleLine": true }],
    "camelcase": 0,
    "comma-style": [1, "last"],
    "comma-spacing": 1,
    "eqeqeq": 0,
    "global-strict": 0,
    "indent": [1, 4],
    "key-spacing": 1,
    "no-alert": 0,
    "no-console": 0,
    "no-empty": 0,
    "no-extra-bind": 1,
    "no-trailing-spaces": 1,
    "no-undef": 1,
    "no-underscore-dangle": 0,
    "no-unused-expressions": 0,
    "no-unused-vars": [1],
    "no-use-before-define": 0,
    "quotes": [0],
    "semi": 1,
    "space-after-function-name": [1, "never"],
    "space-in-brackets": 0,
    "space-in-parens": [1, "never"],
    "strict": 0
  }
}
